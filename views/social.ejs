.story {
  position: relative;
  margin-top: 70px;
  width: 490px;
  height: 150px;
  overflow-x: auto;
}

.containermainstory {
  display: flex;
  flex-direction: row;
  align-items: center;
  column-gap: 20px;
  width: inherit;
  height: 150px;
}

.story::-webkit-scrollbar {
  display: none;
}
.containerstory {
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
  margin-left: 10px;
  min-width: 70px;
  min-height: 70px;
  border-radius: 100%;
  background: url(./images/zuck.jpeg) no-repeat center;
  background-size: cover;
}

.containerstory:nth-child(2) {
  background: url(./images/post1_owner_avatar.jpeg) no-repeat center;
  background-size: cover;
}

.containerstory:nth-child(3) {
  background: url(./images/elonmusk.jpeg) no-repeat center;
  background-size: cover;
}

.containerstory:nth-child(4) {
  background: url(./images/google.jpeg) no-repeat center;
  background-size: cover;
}

.containerstory:nth-child(4) {
  background: url(./images/google.jpeg) no-repeat center;
  background-size: cover;
}

.containerstory:nth-child(5) {
