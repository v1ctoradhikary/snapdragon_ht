  border-radius: 100%;
  margin-right: 15px;
  align-self: baseline;
  margin-top: 18px;
}

.postheaderright p {
  color: rgb(202, 202, 202);
  font-weight: 100;
  font-size: 13px;
  margin-left: 10px;
}

.text1 {
