    Arial, sans-serif;
  user-select: none;
  width: 100vw;
  overflow-x: hidden;
}


body::-webkit-scrollbar {
  background-color: black;
  width: 8px;
}

body::-webkit-scrollbar-thumb {
  background-color: rgba(255, 255, 255, 0.625);
  border-radius: 20px;
  transition: ease-in-out;
}

main {
  max-width: 500px;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  flex-direction: row;
  column-gap: 40px;
}

p,
img,
h5,
h6,
svg,
.story div {
  cursor: pointer;
  color: aliceblue;
}

svg {
  color: aliceblue;
}

/*----------------------SIDE BAR-------------------------*/
nav {
  background-color: black;
  z-index: 1;
}

li img {
  width: 25px;
  height: 25px;
}

li h6 {
  color: aliceblue;
  margin-left: 7px;
  font-size: 12px;
