  background-size: cover;
}

.containerstory:nth-child(6) {
  background: url(./images/instagram.jpeg) no-repeat center;
  background-size: cover;
}

.containerstory:nth-child(7) {
  background: url(./images/post1.jpg) no-repeat center;
  background-size: cover;
}

.containerstory label {
  text-align: center;
  position: relative;
  top: 55px;
  width: 70px;
  margin-left: auto;
  margin-right: auto;
  font-size: small;
}

.gradeint-border {
  background-image: linear-gradient(
