  flex-direction: row;
  align-items: center;
  align-self: flex-start;
}

.posttext {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: flex-start;
}

.posttext h6 {
  position: relative;
  bottom: 40px;
  font-weight: 200;
}

/*--------------IMAGE------------*/

.postmain {
  position: relative;
  top: 30px;
  background-image: url(./images/post1.jpg);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  width: 500px;
  height: 600px;
  border: 1px solid rgba(255, 255, 255, 0.228);
}

.post1 .postmain {
  background-image: url(./images/post2.jpg);
}

.post2 .postmain {
  background-image: url(./images/5873307.jpg);
}

.postbottoms {
  position: relative;
