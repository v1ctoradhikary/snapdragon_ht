    Arial, sans-serif;
  font-size: 15px;
  font-weight: 500;
}

ul {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  row-gap: 3vh;
  margin-top: 8vh;
}

li {
  display: flex;
  flex-direction: row;
  align-items: center;
  cursor: pointer;
  height: 20px;
  width: 2vw;
}

li:nth-child(9) {
