  font-weight: 400;
  display: flex;
  justify-content: flex-start;
  align-items: baseline;
  column-gap: 15px;
}

.book {
  margin-left: auto;
}

.postbottoms img {
  width: 24px;
  height: 24px;
}

/*--------------post comments------------*/
.comments {
  position: relative;
  top: 38px;
  display: flex;
  flex-direction: column;
  column-gap: 10px;
  height: 30px;
}

.commentselement {
  position: relative;
  margin-bottom: 0px;
  margin-top: 20px;
}

.commentinput {
  background-color: rgba(240, 248, 255, 0);
  border: none;
  margin-top: 10px;
  outline: none;
  color: aliceblue;
}

