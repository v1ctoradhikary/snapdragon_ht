    rgba(131, 58, 180, 1) 0%,
    rgba(249, 49, 21, 1) 66%,
    rgba(252, 176, 69, 1) 89%
  );
  position: absolute;
  top: -5px;
  bottom: -5px;
  right: -5px;
  left: -5px;
  z-index: -1;
  border-radius: inherit;
}

/*--------------------------------POST------------------------------------------*/
.post1,
.post2 {
  position: relative;
  margin-top: 230px;
}

.post {
  margin-top: 10px;
}
